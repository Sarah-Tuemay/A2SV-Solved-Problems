class Solution(object):
    def isPalindrome(self, x):
        if x < 0:
            return False
        new_num = 0
        org_num = x
        while x > 0:
            new_num += (x % 10)
            if(x >= 10):
                new_num *= 10
            x /= 10
        
        if new_num == org_num:
            return True
        else:
            return False 
        
